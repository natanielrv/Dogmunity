#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/da0727c9-5f42-4301-ae45-5cde798de4c5/ProyectosCode/Metodologia_de_Analisis/Dogmunity/node_modules/.pnpm/vite@6.4.1_@types+node@20.19.25_jiti@2.6.1_lightningcss@1.30.2_terser@5.44.1/node_modules/vite/bin/node_modules:/mnt/da0727c9-5f42-4301-ae45-5cde798de4c5/ProyectosCode/Metodologia_de_Analisis/Dogmunity/node_modules/.pnpm/vite@6.4.1_@types+node@20.19.25_jiti@2.6.1_lightningcss@1.30.2_terser@5.44.1/node_modules/vite/node_modules:/mnt/da0727c9-5f42-4301-ae45-5cde798de4c5/ProyectosCode/Metodologia_de_Analisis/Dogmunity/node_modules/.pnpm/vite@6.4.1_@types+node@20.19.25_jiti@2.6.1_lightningcss@1.30.2_terser@5.44.1/node_modules:/mnt/da0727c9-5f42-4301-ae45-5cde798de4c5/ProyectosCode/Metodologia_de_Analisis/Dogmunity/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/da0727c9-5f42-4301-ae45-5cde798de4c5/ProyectosCode/Metodologia_de_Analisis/Dogmunity/node_modules/.pnpm/vite@6.4.1_@types+node@20.19.25_jiti@2.6.1_lightningcss@1.30.2_terser@5.44.1/node_modules/vite/bin/node_modules:/mnt/da0727c9-5f42-4301-ae45-5cde798de4c5/ProyectosCode/Metodologia_de_Analisis/Dogmunity/node_modules/.pnpm/vite@6.4.1_@types+node@20.19.25_jiti@2.6.1_lightningcss@1.30.2_terser@5.44.1/node_modules/vite/node_modules:/mnt/da0727c9-5f42-4301-ae45-5cde798de4c5/ProyectosCode/Metodologia_de_Analisis/Dogmunity/node_modules/.pnpm/vite@6.4.1_@types+node@20.19.25_jiti@2.6.1_lightningcss@1.30.2_terser@5.44.1/node_modules:/mnt/da0727c9-5f42-4301-ae45-5cde798de4c5/ProyectosCode/Metodologia_de_Analisis/Dogmunity/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/vite.js" "$@"
else
  exec node  "$basedir/../../bin/vite.js" "$@"
fi
