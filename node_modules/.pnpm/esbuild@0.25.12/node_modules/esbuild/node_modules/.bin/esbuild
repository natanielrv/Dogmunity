#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/da0727c9-5f42-4301-ae45-5cde798de4c5/ProyectosCode/Metodologia_de_Analisis/Dogmunity/node_modules/.pnpm/esbuild@0.25.12/node_modules/esbuild/bin/node_modules:/mnt/da0727c9-5f42-4301-ae45-5cde798de4c5/ProyectosCode/Metodologia_de_Analisis/Dogmunity/node_modules/.pnpm/esbuild@0.25.12/node_modules/esbuild/node_modules:/mnt/da0727c9-5f42-4301-ae45-5cde798de4c5/ProyectosCode/Metodologia_de_Analisis/Dogmunity/node_modules/.pnpm/esbuild@0.25.12/node_modules:/mnt/da0727c9-5f42-4301-ae45-5cde798de4c5/ProyectosCode/Metodologia_de_Analisis/Dogmunity/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/da0727c9-5f42-4301-ae45-5cde798de4c5/ProyectosCode/Metodologia_de_Analisis/Dogmunity/node_modules/.pnpm/esbuild@0.25.12/node_modules/esbuild/bin/node_modules:/mnt/da0727c9-5f42-4301-ae45-5cde798de4c5/ProyectosCode/Metodologia_de_Analisis/Dogmunity/node_modules/.pnpm/esbuild@0.25.12/node_modules/esbuild/node_modules:/mnt/da0727c9-5f42-4301-ae45-5cde798de4c5/ProyectosCode/Metodologia_de_Analisis/Dogmunity/node_modules/.pnpm/esbuild@0.25.12/node_modules:/mnt/da0727c9-5f42-4301-ae45-5cde798de4c5/ProyectosCode/Metodologia_de_Analisis/Dogmunity/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/esbuild" "$@"
else
  exec node  "$basedir/../../bin/esbuild" "$@"
fi
