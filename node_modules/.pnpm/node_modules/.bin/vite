#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/Natan/Dogmunity/node_modules/.pnpm/vite@6.4.1_@types+node@20.1_e981462d47a865ffd23bb5717dac70e6/node_modules/vite/bin/node_modules:/mnt/c/Users/Natan/Dogmunity/node_modules/.pnpm/vite@6.4.1_@types+node@20.1_e981462d47a865ffd23bb5717dac70e6/node_modules/vite/node_modules:/mnt/c/Users/Natan/Dogmunity/node_modules/.pnpm/vite@6.4.1_@types+node@20.1_e981462d47a865ffd23bb5717dac70e6/node_modules:/mnt/c/Users/Natan/Dogmunity/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/Natan/Dogmunity/node_modules/.pnpm/vite@6.4.1_@types+node@20.1_e981462d47a865ffd23bb5717dac70e6/node_modules/vite/bin/node_modules:/mnt/c/Users/Natan/Dogmunity/node_modules/.pnpm/vite@6.4.1_@types+node@20.1_e981462d47a865ffd23bb5717dac70e6/node_modules/vite/node_modules:/mnt/c/Users/Natan/Dogmunity/node_modules/.pnpm/vite@6.4.1_@types+node@20.1_e981462d47a865ffd23bb5717dac70e6/node_modules:/mnt/c/Users/Natan/Dogmunity/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../vite/bin/vite.js" "$@"
else
  exec node  "$basedir/../vite/bin/vite.js" "$@"
fi
